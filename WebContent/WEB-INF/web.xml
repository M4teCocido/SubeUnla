<?xml version = "1.0" encoding = "UTF-8" ?>
<web-app xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xmlns = "http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation = "http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id = "WebApp_ID" version = "3.1" >
<display-name >Sube-Unla</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>ControladorIngresarFichada</servlet-name>
		<servlet-class>controladores.ControladorIngresarFichada</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ControladorListados</servlet-name>
		<servlet-class>controladores.ControladorListados</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ControladorAdministrarSubes</servlet-name>
		<servlet-class>controladores.ControladorAdministrarSubes</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ControladorLogin</servlet-name>
		<servlet-class>controladores.ControladorLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ControladorEstadisticas</servlet-name>
		<servlet-class>controladores.ControladorEstadisticas</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ControladorIngresarFichada</servlet-name>
		<url-pattern>/IngresarFichada</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ControladorListados</servlet-name>
		<url-pattern>/Listados</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ControladorAdministrarSubes</servlet-name>
		<url-pattern>/AdministrarSubes</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ControladorLogin</servlet-name>
		<url-pattern>/ControlLogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ControladorEstadisticas</servlet-name>
		<url-pattern>/Estadisticas</url-pattern>
	</servlet-mapping>
</web-app >